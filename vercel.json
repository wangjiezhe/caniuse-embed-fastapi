{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "uv sync",
  "builds": [
    {
      "src": "app/server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/server.py"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "workers": false
    }
  }
}
